{
  "env": {
    "node": true,
    "jest/globals": true
  },
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "standard", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ]
  }
}
